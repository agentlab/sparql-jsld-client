{
  "name": "@agentlab/sparql-jsld-client",
  "version": "5.0.10",
  "description": "SPARQL JSON Schema Linked Data Client",
  "license": "GPL-3.0",
  "author": "Alexey Ivanov <amivanoff@gmail.com>",
  "contributors": [
    "Alexey Ivanov <amivanoff@gmail.com>",
    "Ivan Shport"
  ],
  "keywords": [
    "sparql",
    "json-ld",
    "shacl-shape",
    "json-schema",
    "rdf",
    "rdf4j",
    "turtle",
    "trig",
    "semantic web",
    "linked data"
  ],
  "homepage": "https://github.com/agentlab/sparql-jsld-client#README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agentlab/sparql-jsld-client.git"
  },
  "bugs": {
    "url": "https://github.com/agentlab/sparql-jsld-client/issues"
  },
  "main": "es/index.mjs",
  "module": "es/index.mjs",
  "typings": "es/index.mjs.d.ts",
  "files": [
    "es",
    "src"
  ],
  "type": "module",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "clean": "rimraf dist es lib",
    "start": "rollup -w",
    "build": "yarn clean && rollup -c",
    "build-tsc": "yarn tsc --project ./tsconfig-build.json",
    "test": "cross-env NODE_ENV=--experimental-vm-modules jest --runInBand",
    "test:ci": "cross-env NODE_ENV=--experimental-vm-modules jest --ci --coverage --maxWorkers=2",
    "lint": "eslint \"{src,stories,test}**/*.{js,jsx,ts,tsx}\"",
    "prepare": "husky && yarn build",
    "format": "./node_modules/.bin/prettier --write \"{src,stories,test}/**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint-staged": "lint-staged"
  },
  "peerDependencies": {
    "axios": ">=1.7.2",
    "dayjs": ">=1.11.12",
    "is-url": ">=1.2.4",
    "jsonld": ">=8.3.2",
    "lodash-es": ">=4.17.21",
    "mobx": ">=6.13.1",
    "mobx-state-tree": ">=6.0.1",
    "rdf-data-factory": ">=1.1.2",
    "rdf-literal": ">=1.3.2",
    "sparqljs": ">=3.7.1",
    "utility-types": ">=3.11.0",
    "uuid62": ">=1.0.2"
  },
  "dependencies": {
    "@types/json-schema": "^7.0.15",
    "@types/jsonld": "^1.5.15",
    "@types/lodash-es": "^4.17.12",
    "@types/sparqljs": "^3.1.11",
    "axios": "^1.7.2",
    "dayjs": "^1.11.12",
    "is-url": "^1.2.4",
    "jsonld": "^8.3.2",
    "lodash-es": "^4.17.21",
    "mobx": "^6.13.1",
    "mobx-state-tree": "^6.0.1",
    "rdf-data-factory": "^1.1.2",
    "rdf-literal": "^1.3.2",
    "sparqljs": "^3.7.1",
    "utility-types": "^3.11.0",
    "uuid62": "^1.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/is-url": "^1.2.32",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "cross-env": "^7.0.3",
    "custom-env": "^2.0.6",
    "esbuild": "^0.23.0",
    "eslint": "9.x",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.8.0",
    "husky": "^9.1.3",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "jest-watch-typeahead": "^2.2.2",
    "lint-staged": "^15.2.7",
    "prettier": "3.3.3",
    "rimraf": "^6.0.1",
    "rollup": "^4.19.1",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.3",
    "typescript": "5.5.4",
    "typescript-eslint": "^7.18.0"
  },
  "lint-staged": {
    "{src,test}/**/*.{js,jsx,ts,tsx,json}": [
      "eslint"
    ]
  },
  "browser": {
    "_process": false,
    "fs": false,
    "path": false
  }
}