    {
      "name": "Debug Jest Tests",
      "type": "node",
      "request": "launch",
      "args": [
        "--inspect-brk",
        "${workspaceRoot}/node_modules/tsdx/dist/index.js",
        "test",
        "--runInBand",
        "--no-cache",
        "--env=jsdom"],
      "cwd": "${workspaceRoot}",
      "protocol": "inspector",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
