{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Jest Tests",
      "type": "node",
      "request": "launch",
      // https://github.com/formium/tsdx/issues/577
      "runtimeArgs": [
        "--inspect-brk",
        "${workspaceRoot}/node_modules/tsdx/dist/index.js",
        "test",
        "--runInBand",
        //"-t", "should return Module UsedInModules with associated Artifact"
        "SparqlGen.spec.ts",
        //"--no-cache",
        //"--env=jsdom"
      ],
      //"cwd": "${workspaceRoot}",
      //"protocol": "inspector",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "name": "vscode-jest-tests",
      "type": "node",
      "request": "launch",
      // https://github.com/formium/tsdx/issues/577
      "runtimeArgs": [
        "--inspect-brk",
        "${workspaceRoot}/node_modules/tsdx/dist/index.js",
        "test",
        "--runInBand",
        //"--no-cache",
        //"--env=jsdom"
      ],
      //"cwd": "${workspaceRoot}",
      //"protocol": "inspector",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "name": "Launch Program",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}\\start"
    }
  ]
}
