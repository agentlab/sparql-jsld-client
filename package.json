    "lodash-es": ">=4.17.21",
    "mobx": ">=6.3.9",
    "mobx-state-tree": ">=5.1.0",
    "moment": ">=2.29.1",
    "rdf-literal": ">=1.3.0",
    "sparqljs": ">=3.5.1",
    "utility-types": ">=3.10.0",
    "uuid62": ">=1.0.2"
  },
  "dependencies": {
    "@types/json-schema": "^7.0.9",
    "@types/lodash-es": "^4.17.5",
