{
  "name": "@agentlab/sparql-jsld-client",
  "version": "5.0.0-rc.8",
  "description": "SPARQL JSON Schema Linked Data Client",
  "license": "GPL-3.0",
  "author": "Alexey Ivanov <amivanoff@gmail.com>",
  "contributors": [
    "Alexey Ivanov <amivanoff@gmail.com>",
    "Ivan Shport"
  ],
  "keywords": [
    "sparql",
    "json-ld",
    "shacl-shape",
    "json-schema",
    "rdf",
    "rdf4j",
    "turtle",
    "trig",
    "semantic web",
    "linked data"
  ],
  "homepage": "https://github.com/agentlab/sparql-jsld-client#README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agentlab/sparql-jsld-client.git"
  },
  "bugs": {
    "url": "https://github.com/agentlab/sparql-jsld-client/issues"
  },
  "main": "es/index.js",
  "module": "es/index.js",
  "typings": "es/index.d.ts",
  "files": [
    "es",
    "src"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "clean": "rimraf dist es lib",
    "start": "rollup -w",
    "build": "yarn clean && rollup -c",
    "build-tsc": "yarn tsc --project ./tsconfig-build.json",
    "test": "cross-env NODE_ENV=--experimental-vm-modules jest",
    "test:ci": "cross-env NODE_ENV=--experimental-vm-modules jest --ci --coverage --maxWorkers=2",
    "lint": "eslint \"{src,stories,test}**/*.{js,jsx,ts,tsx}\"",
    "prepare": "husky install && yarn build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "format": "./node_modules/.bin/prettier --write \"{src,stories,test}/**/*.{js,jsx,ts,tsx,json,css,less,scss,md}\"",
    "lint-staged": "lint-staged"
  },
  "peerDependencies": {
    "@rdfjs/data-model": ">=1.2.0",
    "axios": ">=0.21.1",
    "is-url": ">=1.2.4",
    "jsonld": ">=5.2.0",
    "lodash-es": ">=4.17.21",
    "mobx": ">=6.3.2",
    "mobx-state-tree": ">=5.0.2",
    "moment": ">=2.29.1",
    "rdf-literal": ">=1.2.0",
    "sparqljs": ">=3.4.2",
    "utility-types": ">=3.10.0",
    "uuid62": ">=1.0.1"
  },
  "dependencies": {
    "@types/json-schema": "^7.0.7",
    "@types/lodash-es": "^4.17.4",
    "@types/rdf-js": "^4.0.2",
    "@types/sparqljs": "^3.1.2",
    "@rdfjs/data-model": "^1.2.0",
    "axios": "^0.21.1",
    "is-url": "^1.2.4",
    "jsonld": "^5.2.0",
    "lodash-es": "^4.17.21",
    "mobx": "^6.3.2",
    "mobx-state-tree": "^5.0.2",
    "moment": "^2.29.1",
    "rdf-literal": "^1.2.0",
    "sparqljs": "^3.4.2",
    "utility-types": "^3.10.0",
    "uuid62": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/is-url": "^1.2.29",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.4",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "custom-env": "^2.0.1",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-flowtype": "^5.7.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "jest": "^27.0.5",
    "jest-extended": "^0.11.5",
    "jest-watch-typeahead": "^0.6.4",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^27.0.3",
    "tslib": "^2.3.0",
    "typescript": "^4.3.4"
  },
  "lint-staged": {
    "{src,stories,test}/**/*.{js,jsx,ts,tsx,json}": [
      "eslint"
    ],
    "{src,stories,test}/**/*.{css,less,scss,md}": [
      "prettier --write"
    ]
  },
  "browser": {
    "_process": false,
    "fs": false,
    "path": false
  }
}
